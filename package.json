{
  "name": "scolink-backend",
  "version": "1.0.0",
  "description": "Backend API for Scolink Application",
  "main": "docker-start.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "rm -rf dist && tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json && prisma generate",
    "start": "node docker-start.js",
    "start:compiled": "node -r tsconfig-paths/register dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node database/seeds/index.ts",
    "db:seed:comprehensive": "npm run build && node dist/database/seeds/comprehensive-seed.js",
    "db:seed:full": "node scripts/run-comprehensive-seed.js",
    "db:wipe": "node scripts/wipe-database.js",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@types/supertest": "^2.0.16",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "cross-env": "^10.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.8",
    "ttypescript": "^1.5.15"
  },
  "keywords": [
    "scolink",
    "education",
    "api",
    "backend"
  ],
  "author": "Scolink Team",
  "license": "MIT"
}
